alues\n","    m_curr = b_curr = 0\n","    iterations = 100\n","    n = len(x)\n","    learning_rate = 0.00001\n","    prev_cost = 0\n","\n","    for i in range(iterations):\n","        y_predicted = m_curr * x + b_curr\n","        cost = (1/n) * sum([val**2 for val in (y - y_predicted)])\n","\n","        if math.isclose(prev_cost, cost, abs_tol=1e-20):\n","            print(f\"Stopping early at iteration {i} due to cost convergence.\")\n","            break\n","\n","        md = -(2/n)*sum(x * (y - y_predicted))\n","        bd = -(2/n)*sum(y - y_predicted)\n","        m_curr = m_curr - learning_rate * md\n","        b_curr = b_curr - learning_rate * bd\n","        prev_cost = cost\n","\n","        print(f\"m {m_curr}, b {b_curr}, cost {cost}, iteration {i}\")\n","\n","    return m_curr, b_curr\n","m, b = gradient_descent_from_df(df)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"eTL0NSaGnoCz","executionInfo":{"status":"ok","timestamp":1752929076468,"user_tz":-300,"elapsed":27,"user":{"displayName":"Amna Miraj","userId":"06176858818659952799"}},"outputId":"e5a7f141-6426-4bd2-d319-f207048546d4"},"execution_count":27,"outputs":[{"output_type":"stream","name":"stdout","text":["m 0.09891800000000002, b 0.0013980000000000002, cost 5199.1, iteration 0\n","m 0.188468576032, b 0.002663817592, cost 4266.767068953651, iteration 1\n","m 0.2695388123929874, b 0.003809970298069408, cost 3502.654811589925, iteration 2\n","m 0.3429317875153011, b 0.004847790245306415, cost 2876.4112000801774, iteration 3\n","m 0.4093745292255847, b 0.005787536421381067, cost 2363.1606178664038, iteration 4\n","m 0.46952521664984737, b 0.006638496299607258, cost 1942.5157516977667, iteration 5\n","m 0.5239797001063835, b 0.00740907784023707, cost 1597.7677678205928, iteration 6\n","m 0.5732774035723166, b 0.008106892779338137, cost 1315.2226045420603, iteration 7\n","m 0.6179066681933365, b 0.008738832030309935, cost 1083.6570174148635, iteration 8\n","m 0.658309589769199, b 0.